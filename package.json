    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "6.2.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "handlebars": "^4.0.12",
    "winston": "^3.1.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "test/app.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
