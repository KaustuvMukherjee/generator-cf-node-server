{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["warn", 4],
        "quotes": ["warn", "single"],
        "semi": ["warn", "never"],
        "no-console": ["off"],
        "no-unused-vars": ["off"]
    }
}
